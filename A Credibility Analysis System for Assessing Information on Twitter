<script>
bds.ready(function() {
    var $sortwr = $('.sortwr'),
        $filterBtn = $('.sortwr .filter_btn'),
        $ul = $sortwr.find('.sortlist'),
        $a = $ul.find('a');

    $filterBtn.on('touchend', function(event) {
        $sortwr.toggleClass('sortopen');
        if ($sortwr.hasClass('sortopen')) {
            showBg();
            ns_c_xs({
                'fm' : 'beha',
                'button_tp' : "show_filter"
            });
        } else {
            $('.w_sort_bgcover').hide();
            ns_c_xs({
                'fm' : 'beha',
                'button_tp' : "hide_filter"
            });
        }
    });
    function showBg () {
        if (!$('.w_sort_bgcover').length) {
            var top = $('#page-top').height() * 1 + $('#page-nav').height() * 1;
            var html = '<div class="w_sort_bgcover" style="top:' + top + 'px;"></div>';
            $(html).appendTo($('body'));
        }
        var h = Math.max($(window).height(), $('body').height());
        $('.w_sort_bgcover').css({
            height: h,
            display: 'block'
        });
    }
    $('body').on('touchend', '.w_sort_bgcover', function(event) {
        $sortwr.removeClass('sortopen');
        $('.w_sort_bgcover').hide();

        // 防止事件透传
        event.preventDefault();
        event.stopPropagation();
    });
})
</script>
